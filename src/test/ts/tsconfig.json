{
    "version": "1.5.0-beta",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "../../../target/classes"
    },
    "filesGlob": [
        "./src/**/*.ts",
        "./typings/**/*.ts",
        "./../../../target/classes/model.d.ts"
    ],
    "files": [
        "./src/test.ts",
        "./../../../target/classes/model.d.ts"
    ]
}
